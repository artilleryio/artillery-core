    "eslint": "^0.24.0",
    "express": "4.13.4",
    "good": "6.4.0",
    "good-console": "5.1.0",
    "hapi": "10.4.0",
    "hapi-auth-basic": "4.1.0",
    "istanbul": "^0.3.17",
    "nock": "^9.0.2",
    "pre-commit": "^1.0.10",
    "sinon": "1.17.6",
    "tape": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoreditch-ops/artillery-core.git"
